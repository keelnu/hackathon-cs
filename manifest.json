{
  "name": "Kristiina and Jenny's Excellent Extensions!",
  "version": "1.0",
  "description": "Build an Extension!",
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },

  "permissions": [
    "alarms",
    "storage",
    "declarativeContent",
    "activeTab",
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  // "icons": {
  //   "16": "images/get_started16.png",
  //   "32": "images/get_started32.png",
  //   "48": "images/get_started48.png",
  //   "128": "images/get_started128.png"
  // },
  "manifest_version": 2
}
